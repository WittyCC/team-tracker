<h1>Add a new member to team $team.getTeamName()</h1>

<form action="/members" method="post">
  <input id="teamId" name="teamId" type="hidden" value="$team.getId()">

  <div class="form-group">
  <label for="name">Name of New Member</label>
  <input id="name" name="name" type="text">
  </div>

  <button type="submit" class="btn btn-warning btn-lg">Register</button>
</form>
